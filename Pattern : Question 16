#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     n=4
     D 
     C D 
     B C D 
     A B C D  
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    char ch;
    for(int i=1;i<=n;i++){
        ch='A'+n-i;
        for(int j=1;j<=i;j++){ 
            cout<<ch<<" ";  
            ch+=1;
        }
        cout<<endl; 
       
    }  
}
