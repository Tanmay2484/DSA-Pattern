#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     n=3
     A 
     B B
     C C C 
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    char ch;
    for(int i=0;i<n;i++){
        for(int j=0;j<i+1;j++){ 
            ch='A'+i;
            cout<<ch<<" ";  
        }
        cout<<endl; 
       
    }  
}
