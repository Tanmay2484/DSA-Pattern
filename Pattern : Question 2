#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     4 3 2 1
     4 3 2 1
     4 3 2 1
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    int i=1,j;
    while(i<=n){
        j=1;
        while(j<=n){
            cout<<n-j+1<<" ";
            j++;
        }
        cout<<endl;
        i++;
    }

}
