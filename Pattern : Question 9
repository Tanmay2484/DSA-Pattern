#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     n=4
     1
     2 1
     3 2 1
     4 3 2 1
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=i;j>=1;j--){
            cout<<j<<" ";
        }
        cout<<endl;
    }

}
