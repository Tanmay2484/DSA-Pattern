#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     n=4
     A 
     B C 
     D E F 
     G H I J 
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    char ch;
    int m=0;
    for(int i=0;i<n;i++){
        for(int j=0;j<i+1;j++){ 
            ch='A'+m;
            cout<<ch<<" ";
            m++;  
        }
        cout<<endl; 
       
    }  
}
