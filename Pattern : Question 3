#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     n=3
     1 2 3
     4 5 6
     7 8 9
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    int i=1,j,count=1;
    while(i<=n){
        j=1;
        while(j<=n){
            cout<<count<<" ";
            j++;
            count++;
        }
        cout<<endl;
        i++;
    }

}
//another ways
#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     n=3
     1 2 3
     4 5 6
     7 8 9
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    int count=1;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            cout<<count<<" ";
            count++;
        }
        cout<<endl;
    }

}
