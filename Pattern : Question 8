#include <bits/stdc++.h>
using namespace std;
/* Pattern like this
     n=4
     1
     2 3
     3 4 5
     4 5 6 7
*/
int main() {
    //here row is n and column is 1 to n
    int n;
    //cout<<"enter the value of row : ";
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=0;j<i;j++){
            cout<<i+j<<" ";//where starting from i=1 and j=0
        }
        cout<<endl;
    }

}
